  "scripts": {
    "build": "yarn add sqlite3 sequelize-cli -D && sequelize db:migrate:undo:all&&sequelize db:migrate&&yarn sequelize-cli db:seed:all",
    "prestart": "sequelize db:migrate:undo:all&&sequelize db:migrate&&yarn sequelize-cli db:seed:all",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js --ignore tests",
    "pretest": "set NODE_ENV=test&&sequelize db:migrate:undo:all&&sequelize db:migrate",
    "test": "set NODE_ENV=test&&jest",
    "posttest": "set NODE_ENV=test&&sequelize db:migrate:undo:all",
    "seed": "sequelize db:migrate:undo:all&&sequelize db:migrate&&yarn sequelize-cli db:seed:all"
  },
  "dependencies": {
    "cpf-cnpj-validator": "^1.0.3",
    "dotenv": "^8.2.0",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.17.1",
    "faker-br": "^0.4.1",
    "helmet": "^4.4.1",
    "jest-each": "^26.6.2",
    "json-schema-faker": "^0.5.0-rcv.34",
    "jsonpath": "^1.1.1",
    "jsonschema": "^1.4.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "2.22.1",
    "factory-girl": "^5.0.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
}
